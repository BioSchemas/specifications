                    {
                        "$ref": "#/definitions/organization"
                    },
                    {
                        "type": "array",
                        "items": {
                            "$ref": "#/definitions/organization"
                        }
                    }
                  ]
              },
              "identifier": {
                "type": "string"
              }
            }
          },
          "organization": {
            "@type": "Organization",
