                            "description": {
                                "type": "string"
                            },
                            "sameAs": {
                                "type": "string",
                                "format": "uri"
                            }
                        }
                    },
                    "person": {
                        "@type": "Person",
                        "type": "object",
                        "properties": {
                            "name": {
