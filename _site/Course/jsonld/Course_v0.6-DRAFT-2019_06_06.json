                                        "type": "array",
                                        "items": {
                                            "type": "string",
                                            "format": "uri"
                                        }
                                    },
                                    {
                                        "$ref": "#/definitions/creativework"
                                    },
                                    {
                                        "type": "array",
                                        "items": {
                                            "$ref": "#/definitions/creativework"
                                        }
                                    }
                                ]
                            }
                        },
                        "required": [
                            "name"
                        ]
                    },
                    "courseinstance": {
