                    },
                    "imageobject": {
                        "@type": "ImageObject",
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "url": {
                                "type": "string",
                                "format": "uri"
                            }
                        },
                        "required": []
                    },
                    "product": {
                        "@type": "schema:Product",
