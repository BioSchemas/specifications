                    },
                    "course": {
                        "@type": "bioschemas:Course",
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            }
                        },
                        "required": []
                    },
                    "alignmentobject": {
                        "@type": "schema:AlignmentObject",
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            }
                        },
                        "required": []
                    },
                    "organization": {
                        "@type": "Organization",
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "legalName": {
                                "type": "string"
                            },
                            "description": {
                                "type": "string"
                            },
                            "sameAs": {
                                "type": "string",
                                "format": "uri"
                            }
                        }
                    },
                    "person": {
                        "@type": "Person",
