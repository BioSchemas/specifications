                    "alternateName",
                    "audience",
                    "comment",
                    "commentCount",
                    "courseCode",
                    "creator",
                    "dateCreated",
                    "dateModified",
                    "educationalCredentialAwarded",
                    "hasPart",
                    "image",
                    "isBasedOn",
                    "thumbnailUrl"
                ],
