                        },
                        "required": []
                    },
                    "propertyvalue": {
                        "@type": "PropertyValue",
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string"
                            },
                            "value": {
                                "oneOf": [
                                    {
                                        "type": "string"
