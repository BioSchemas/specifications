                                "items": {
                                    "type": "string",
                                    "format": "uri"
                                }
                            }
                        ],
                        "owl:cardinality": "many"
                    },
                    "hasMolecularFunction": {
                        "description": "GO term corresponding to the Molecular Function branch of the GO ontology.",
                        "anyOf": [
                            {
                                "$ref": "#/definitions/definedTerm"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "$ref": "#/definitions/definedTerm"
                                }
                            },
                            {
                                "$ref": "#/definitions/propertyValue"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "$ref": "#/definitions/propertyValue"
