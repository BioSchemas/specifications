                            {
                                "$ref": "#/definitions/propertyValue"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "$ref": "#/definitions/propertyValue"
                                }
                            },
                            {
                                "type": "string"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            },
                            {
                                "type": "string",
                                "format": "uri"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "format": "uri"
                                }
                            }
                        ],
                        "owl:cardinality": "many"
                    },
