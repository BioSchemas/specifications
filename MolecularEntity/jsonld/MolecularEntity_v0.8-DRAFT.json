                "properties": {
                    "inChI": {
                        "description": "Non-proprietary identifier for molecular entity that can be used in printed and electronic data sources thus enabling easier linking of diverse data compilations.\n\nTo comply with the 'FAIR chemical structures' specification, at least one of inChI, inChIKey, iupacName, and smiles should be provided.",
                        "type": "string",
                        "owl:cardinality": "one"
                    },
                    "inChIKey": {
                        "description": "InChIKey is a hashed version of the full InChI (using the SHA-256 algorithm).\n\nTo comply with the 'FAIR chemical structures' specification, at least one of inChI, inChIKey, iupacName, and smiles should be provided.",
                        "type": "string",
                        "owl:cardinality": "one"
                    },
                    "iupacName": {
